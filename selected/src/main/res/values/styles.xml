<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>

        <item name="android:fitsSystemWindows">true</item>
        <item name="android:windowTranslucentStatus">false</item>
        <item name="android:windowTranslucentNavigation">false</item>
    </style>

    <style name="multimedia_selected_folder_window" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/multimedia_selected_folder_window_enter</item>  <!--//进入时的动画-->
        <item name="android:windowExitAnimation">@anim/multimedia_selected_folder_window_exit</item>    <!--//退出时的动画-->
    </style>
</resources>